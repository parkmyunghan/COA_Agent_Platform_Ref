        "선제방책"@ko ;
    rdfs:subClassOf ns1:COA .

ns1:appliesTo a owl:ObjectProperty ;
    rdfs:domain ns1:제약조건 ;
    rdfs:range ns1:아군부대현황,
        ns1:임무정보,
        ns1:적군부대현황,
        ns1:전장축선,
        ns1:지형셀 .

ns1:attackPower a owl:DatatypeProperty ;
    rdfs:label "공격력"@ko ;
    rdfs:domain ns1:OffensiveCOA ;
    rdfs:range xsd:float .

ns1:breakthroughCapability a owl:DatatypeProperty ;
    rdfs:label "돌파능력"@ko ;
    rdfs:domain ns1:OffensiveCOA ;
    rdfs:range xsd:float .

ns1:defenseCoverage a owl:DatatypeProperty ;
    rdfs:label "방어범위"@ko ;
    rdfs:domain ns1:DefenseCOA ;
    rdfs:range xsd:string .

ns1:defenseStrength a owl:DatatypeProperty ;
    rdfs:label "방어강도"@ko ;
    rdfs:domain ns1:DefenseCOA ;
    rdfs:range xsd:float .

ns1:effectiveness a owl:DatatypeProperty ;
    rdfs:label "효과성"@ko ;
    rdfs:domain ns1:COA ;
    rdfs:range xsd:float .

ns1:hasMission a owl:ObjectProperty ;
    rdfs:domain ns1:아군부대현황 ;
    rdfs:range ns1:임무정보 .

ns1:has임무정보 a owl:ObjectProperty ;
    rdfs:domain ns1:위협상황 ;
    rdfs:range ns1:임무정보 .

ns1:has적군부대현황 a owl:ObjectProperty ;
    rdfs:domain ns1:위협상황 ;
    rdfs:range ns1:적군부대현황 .

ns1:has전장축선 a owl:ObjectProperty ;
    rdfs:domain ns1:아군부대현황,
        ns1:위협상황,
        ns1:임무정보,
        ns1:적군부대현황 ;
    rdfs:range ns1:전장축선 .

ns1:has지형셀 a owl:ObjectProperty ;
    rdfs:domain ns1:위협상황,
        ns1:전장축선 ;
    rdfs:range ns1:지형셀 .

ns1:locatedIn a owl:ObjectProperty ;
    rdfs:domain ns1:아군부대현황,
        ns1:적군부대현황 ;
    rdfs:range ns1:지형셀 .

ns1:relatedTo a owl:ObjectProperty ;
    rdfs:domain ns1:방책템플릿,
        ns1:적군부대현황 ;
    rdfs:range ns1:위협상황,
        ns1:임무정보 .

