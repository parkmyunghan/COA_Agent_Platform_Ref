ns:COA_Library_COA_CNT_001 a ns:COA,
        ns:COA_Library,
        ns:CounterAttackCOA ;
    rdfs:label "충격 반격" ;
    ns:compatibilityScore "0.2"^^xsd:float,
        "1.0"^^xsd:float ;
    ns:compatibleWith ns:맑음,
        ns:평지 ;
    ns:hasConstraint ns:없음 ;
    ns:hasPhasingInfo "Phase 1" ;
